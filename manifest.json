{
  "manifest_version": 3,
  "name": "Text Changer",
  "version": "1.0",
  "description": "Change text on specific URLs.",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://example.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://example.com/*"],
      "js": ["content.js"]
    }
  ]
}
